{
  "name": "@react/experimental-s2",
  "version": "0.1.0",
  "description": "Spectrum 2 UI components in React",
  "repository": {
    "type": "git",
    "url": "https://git.corp.com/react/experimental-s2"
  },
  "main": "dist/main.cjs",
  "module": "dist/module.mjs",
  "types": "dist/types.d.ts",
  "page": "dist/page.cjs",
  "style-module": "style/dist/module.mjs",
  "style-main": "style/dist/main.cjs",
  "style-types": "style/dist/types.d.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "module": "./dist/module.mjs",
      "import": "./dist/module.mjs",
      "require": "./dist/main.cjs"
    },
    "./page.css": "./page.css",
    "./style": {
      "types": "./style/dist/types.d.ts",
      "module": "./style/dist/module.mjs",
      "import": "./style/dist/module.mjs",
      "require": "./style/dist/main.cjs"
    },
    "./icons/*": {
      "types": "./icon.d.ts",
      "module": "./icons/*.mjs",
      "import": "./icons/*.mjs",
      "require": "./icons/*.cjs"
    }
  },
  "targets": {
    "module": {},
    "main": {},
    "types": {},
    "page": {
      "source": "src/page.ts",
      "isLibrary": true
    },
    "style-module": {
      "source": "style/spectrum-theme.ts",
      "isLibrary": true,
      "outputFormat": "esmodule",
      "context": "node"
    },
    "style-main": {
      "source": "style/spectrum-theme.ts",
      "isLibrary": true,
      "outputFormat": "commonjs",
      "context": "node"
    },
    "style-types": {
      "source": "style/spectrum-theme.ts"
    },
    "icons-module": {
      "source": "s2wf-icons/assets/svg/*.svg",
      "distDir": "icons",
      "isLibrary": true,
      "outputFormat": "esmodule",
      "includeNodeModules": false
    },
    "icons-main": {
      "source": "s2wf-icons/assets/svg/*.svg",
      "distDir": "icons",
      "isLibrary": true,
      "outputFormat": "commonjs",
      "includeNodeModules": false
    }
  },
  "browserslist": "last 2 Chrome versions, last 2 Safari versions, last 2 Firefox versions, last 2 Edge versions",
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist",
    "style/dist",
    "style/package.json",
    "page.css",
    "icons",
    "icon.d.ts"
  ],
  "scripts": {
    "check-types": "tsc",
    "start": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-storybook-chromatic": "storybook build --test",
    "storybook": "storybook dev -p 6006",
    "lint": "concurrently \"yarn check-types\" \"eslint --ext .js,.jsx,.ts,.tsx .\"",
    "postinstall": "patch-package",
    "build": "parcel build && rm -rf dist/page.cjs* && mv dist/page*.css page.css",
    "clean-build": "rm -rf dist style/dist page.css icons .parcel-cache",
    "prepare": "husky",
    "chromatic": "chromatic --buildScriptName='build-storybook-chromatic' --project-token=$RAINBOW_CHROMATIC_PROJECT_TOKEN",
    "storybook-docs": "storybook dev --docs",
    "build-storybook-docs": "storybook build --docs",
    "prepack": "npm pkg delete scripts devDependencies resolutions alias targets",
    "postpack": "git checkout -- package.json",
    "api-diff": "npm pkg set apiCheck=dist/api.json && npm pkg set targets.apiCheck.context=browser && parcel build --target apiCheck --config .parcelrc-api && node scripts/api-diff.js --skip-style-props --skip-same && npm pkg delete apiCheck && npm pkg delete targets.apiCheck"
  },
  "devDependencies": {
    "@adobe/spectrum-tokens": "^13.0.0-beta.34",
    "@parcel/bundler-library": "2.11.1-canary.3165",
    "@parcel/config-default": "2.0.0-canary.1542",
    "@parcel/macros": "2.12.1-canary.3165",
    "@parcel/packager-ts": "2.0.0-canary.1542",
    "@parcel/transformer-svg-react": "2.12.1-canary.3165",
    "@parcel/transformer-typescript-types": "2.0.0-canary.1542",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-themes": "^7.5.3",
    "@storybook/addons": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/client-api": "^7.5.3",
    "@storybook/manager-api": "^7.5.3",
    "@storybook/preview-api": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-webpack5": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@svgr/core": "^8.0.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.0.0",
    "@swc/core": "^1.4.0",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/jscodeshift": "^0.11.11",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "chromatic": "^11.0.8",
    "colorjs.io": "^0.4.5",
    "concurrently": "^8.2.2",
    "eslint": "^8.38.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsdoc": "^40.3.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-monorepo": "^0.3.2",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-rulesdir": "^0.2.2",
    "fast-glob": "^3.2.6",
    "glob-promise": "^6.0.5",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jscodeshift": "^0.15.1",
    "parcel": "2.0.0-canary.1540",
    "parcel-resolver-storybook": "https://gitpkg.now.sh/mischnic/storybook-parcel/packages/parcel-resolver-storybook?master",
    "parcel-transformer-mdx": "^0.3.2",
    "patch-package": "^8.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-docgen-typescript": "^2.2.2",
    "react-docgen-typescript-plugin": "^1.0.5",
    "react-dom": "^18.2.0",
    "sharp": "^0.31.1",
    "storybook": "^7.5.3",
    "storybook-builder-parcel": "https://gitpkg.now.sh/mischnic/storybook-parcel/packages/storybook-builder-parcel?master",
    "storybook-dark-mode": "^3.0.3",
    "storybook-react-parcel": "https://gitpkg.now.sh/mischnic/storybook-parcel/packages/storybook-react-parcel?master",
    "svgo": "^2.3.0",
    "typescript": "^5.3.2"
  },
  "resolutions": {
    "jackspeak": "2.1.1",
    "recast": "0.23.4",
    "ast-types": "0.16.1",
    "@storybook/core-common": "7.6.10"
  },
  "alias": {
    "@storybook/react-dom-shim": "@storybook/react-dom-shim/dist/react-18"
  },
  "dependencies": {
    "@react-aria/i18n": "^3.11.0",
    "@react-aria/utils": "^3.23.0",
    "@react-spectrum/utils": "^3.11.3",
    "@react-types/dialog": "^3.5.8",
    "@react-types/provider": "^3.7.2",
    "@react-types/shared": "^3.22.1",
    "@react-types/textfield": "^3.9.1",
    "csstype": "^3.0.2",
    "react-aria": "^3.31.1",
    "react-aria-components": "^1.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
