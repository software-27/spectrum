{
  "name": "@react-spectrum/rainbow",
  "private": true,
  "version": "4.0.0",
  "description": "Spectrum 2 UI components in React",
  "repository": {
    "type": "git",
    "url": "https://git.corp.com/react/rainbow"
  },
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "page": "dist/page.js",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "module": "./dist/module.js",
      "import": "./dist/import.mjs",
      "require": "./dist/main.js"
    },
    "./page.css": "./dist/page.css"
  },
  "targets": {
    "module": {},
    "main": {},
    "types": {},
    "page": {
      "source": "src/page.ts",
      "isLibrary": true
    }
  },
  "browserslist": "last 2 Chrome versions, last 2 Safari versions, last 2 Firefox versions, last 2 Edge versions",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "check-types": "tsc",
    "start": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "lint": "concurrently \"yarn check-types\" \"eslint --ext .js,.jsx,.ts,.tsx .\"",
    "build-icons": "node scripts/generateIcons.mjs",
    "build:icons": "node scripts/buildIcons.mjs",
    "postinstall": "patch-package && yarn build:icons",
    "build": "parcel build && rm -rf dist/page.js*",
    "prepare": "husky"
  },
  "devDependencies": {
    "@adobe/spectrum-tokens": "^13.0.0-beta.9",
    "@parcel/config-default": "2.0.0-nightly.1498",
    "@parcel/packager-ts": "2.0.0-nightly.1498",
    "@parcel/transformer-svg-react": "2.11.1-nightly.3121",
    "@parcel/transformer-typescript-types": "2.0.0-nightly.1498",
    "@react-aria/utils": "^3.23.0",
    "@react-spectrum/utils": "^3.11.3",
    "@storybook/addon-designs": "^7.0.5",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-themes": "^7.5.3",
    "@storybook/addons": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/client-api": "^7.5.3",
    "@storybook/manager-api": "^7.5.3",
    "@storybook/preview-api": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-webpack5": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@swc/core": "^1.4.0",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/jscodeshift": "^0.11.11",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "colorjs.io": "^0.4.5",
    "eslint": "^8.38.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsdoc": "^40.3.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-monorepo": "^0.3.2",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-rulesdir": "^0.2.2",
    "fast-glob": "^3.2.6",
    "glob-promise": "^6.0.5",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jscodeshift": "^0.15.1",
    "parcel": "2.0.0-nightly.1496",
    "parcel-resolver-storybook": "https://gitpkg.now.sh/mischnic/storybook-parcel/packages/parcel-resolver-storybook?master",
    "patch-package": "^8.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-docgen-typescript": "^2.2.2",
    "react-docgen-typescript-plugin": "^1.0.5",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.3",
    "storybook-builder-parcel": "https://gitpkg.now.sh/mischnic/storybook-parcel/packages/storybook-builder-parcel?master",
    "storybook-dark-mode": "^3.0.3",
    "storybook-react-parcel": "https://gitpkg.now.sh/mischnic/storybook-parcel/packages/storybook-react-parcel?master",
    "typescript": "^5.3.2"
  },
  "resolutions": {
    "jackspeak": "2.1.1",
    "recast": "0.23.4",
    "ast-types": "0.16.1",
    "@storybook/core-common": "7.6.10"
  },
  "alias": {
    "@storybook/react-dom-shim": "@storybook/react-dom-shim/dist/react-18"
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.34.0",
    "@react-aria/utils": "^3.23.0",
    "@react-spectrum/utils": "^3.11.3",
    "concurrently": "^8.2.2",
    "react-aria": "^3.31.1",
    "react-aria-components": "^1.1.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
