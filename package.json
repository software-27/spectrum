  "@parcel/transformer-css": {
    "cssModules": {
      "global": true,
      "exclude": [
        "**/*.global.css",
        "packages/@react-aria/example-theme/**",
        "packages/@react-spectrum/style-macro-s1/**"
      ]
    },
    "drafts": {
      "nesting": true
    },
    "pseudoClasses": {
      "focusVisible": "focus-ring",
      "hover": "is-hovered"
    }
  }
